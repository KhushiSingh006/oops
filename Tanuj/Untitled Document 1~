#include<stdio.h>
int swap(int,int);
int main()
{
int a,b;
printf("Enter the value of a \n");
scanf("%d ",&a);
printf("Enter the value of b \n");
scanf("%d ",&b);
swap(a,b)
return 0;
}
swap(int x,int y)
{
int x,y,temp;
printf("Values of x and y before swapping are %d and %d \n",x,y);
temp=x;
x=y;
y=temp;
printf("Values of x and y after swapping are %d and %d \n",x,y);
}



